{
  "name": "quickstart",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "server-dev": "npm run dev-server",
    "start": "node dist/app.js",
    "build": "tsc -p .",
    "dev": "npm run stop && concurrently \"npm run dev-server\" \"npm run expose\"",
    "dev-server": "ts-node-dev --watch ./.env ./src/app.ts",
    "stop": "kill-port 8302 && kill-port 4049 && kill-port 4040",
    "test": "jest"
  },
  "dependencies": {
    "@babel/preset-env": "^7.24.7",
    "axios": "^1.6.2",
    "babel-jest": "^29.7.0",
    "express": "^4.18.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^8.5.0",
    "concurrently": "^8.2.0",
    "cross-port-killer": "^1.2.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "reports",
          "outputName": "junit.xml"
        }
      ]
    ]
  }
}