{
  "name": "quickstart",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "server-dev": "npm run dev-server",
    "start": "node dist/app.js",
    "build": "tsc -p .",
    "dev": "npm run stop && concurrently \"npm run dev-server\" \"npm run expose\"",
    "dev-server": "ts-node-dev --watch ./.env ./app.ts",
    "stop": "kill-port 8302 && kill-port 4049 && kill-port 4040"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "express": "^4.18.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "concurrently": "^8.2.0",
    "cross-port-killer": "^1.2.1",
    "nodemon": "^3.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.1.3"
  }
}
